{
  "version": 2,
  "name": "machisaga-multi",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "outputDirectory": "public",
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/mobara",
      "dest": "/index.html"
    },
    {
      "src": "/masuya",
      "dest": "/masuya/index.html"
    },
    {
      "src": "/masuya/(.*)",
      "dest": "/masuya/$1"
    }
  ]
}
