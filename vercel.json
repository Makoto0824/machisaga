{
  "version": 2,
  "name": "machisaga-multi",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/",
      "destination": "/mobara_sandbox/index.html"
    },
    {
      "source": "/mobara",
      "destination": "/mobara_sandbox/index.html"
    },
    {
      "source": "/masuya",
      "destination": "/masuya/index.html"
    },
    {
      "source": "/mobara_sandbox/:path*",
      "destination": "/mobara_sandbox/:path*"
    },
    {
      "source": "/masuya/:path*",
      "destination": "/masuya/:path*"
    }
  ]
}
