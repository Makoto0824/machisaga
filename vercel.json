{
  "version": 2,
  "name": "machisaga-multi",
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/mobara_sandbox/index.html"
    },
    {
      "src": "/mobara",
      "dest": "/mobara_sandbox/index.html"
    },
    {
      "src": "/masuya",
      "dest": "/masuya/index.html"
    },
    {
      "src": "/mobara_sandbox/(.*)",
      "dest": "/mobara_sandbox/$1"
    },
    {
      "src": "/masuya/(.*)",
      "dest": "/masuya/$1"
    }
  ]
}
