{
  "version": 2,
  "name": "dragon-quest-battle-simulator",
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "functions": {},
  "headers": [
    {
      "source": "**/*.css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "**/*.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
